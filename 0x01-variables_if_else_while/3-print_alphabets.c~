#include <stdio.h>
/**
 * main - main block
 * Description: Get a random number and print the number
 * and if it is positive, negative, or zero
 * Return: 0
 */
int main(void)
{
	char x;

	x = 'a';
	while (x <= 'z')
	{
	putchar(x);
	x++;
	}
	x = 'A';
	while (x <= 'Z')
	{
	  putchar(x);
	  x++;
	}

	putchar('\n');
	return (0);
}

